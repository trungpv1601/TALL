{
    "name": "trungpv1601/TALL",
    "description": "Inspire from Laravel Nova",
    "keywords": [
        "trungpv1601",
        "TALL"
    ],
    "homepage": "https://github.com/trungpv1601/TALL",
    "license": "MIT",
    "authors": [
        {
            "name": "trungpv1601",
            "email": "trungpv1601@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "livewire/livewire": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Trungpv1601\\TALL\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trungpv1601\\TALL\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trungpv1601\\TALL\\TALLServiceProvider"
            ],
            "aliases": {
                "TALL": "Trungpv1601\\TALL\\TALLFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
